{
  "name": ",ydsl",
  "description": "mydsl",
  "author": "Microsoft Corporation",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "Sven Marquardt",
  "engines": {
    "vscode": "^1.4.0"
  },
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:mydsl"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "mydsl",
        "aliases": [
          "mydsl"
        ],
        "extensions": [
          ".mydsl"
        ],
        "configuration": "./mydsl.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mydsl",
        "scopeName": "text.mydsl",
        "path": "./mydsl.tmLanguage"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "typescript": "^2.1.4",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "vscode-languageclient": "^2.6.3"
  }
}
